    "node": ">=14.0.0"
  },
  "scripts": {
    "dev": "vite --config vite.config.dev.ts",
    "build:staging": "vite build --mode staging -c vite.config.prod.ts",
    "build": "vite build -c vite.config.prod.ts",
    "report": "cross-env REPORT=true npm run build",
    "preview": "vite preview --host",
    "preview-https": "serve dist",
    "typecheck": "vue-tsc --noEmit --skipLibCheck",
    "up": "taze -I major",
    "prepare": "simple-git-hooks",
    "lint": "eslint .",
    "mock": "apipost-mock-gen fixtures/mock/weila.md --output generated/mock/weila.ts",
    "zip": "esno scripts/zip.ts",
    "test": "vitest",
    "release": "npx bumpp",
    "pack": "nr release && nr build && nr zip "
  },
