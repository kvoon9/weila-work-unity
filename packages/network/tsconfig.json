{
  "compilerOptions": {
    "target": "ESNext",
    "lib": ["esnext", "DOM"],
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "strict": true,
    "declaration": true,
    "declarationDir": "dist",
    "noEmit": true,
    "stripInternal": true,
    "esModuleInterop": true,
    "skipLibCheck": true
  }
}
