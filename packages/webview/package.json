  "private": true,
  "packageManager": "pnpm@9.15.4",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt prepare && nuxt generate",
    "generate:zip": "nuxt prepare && nuxt generate && nr zip",
    "generate:preview": "nr generate:zip && nr preview",
    "prepare": "nuxt prepare",
    "preview": "nuxt preview --host 3000",
    "postinstall": "nr prepare",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "up": "npx taze -I major",
    "typecheck": "tsc --noEmit",
    "zip": "esno ./scripts/zip.ts",
    "serve": "nlx http-server .output/public -a 192.168.0.152 -p 3000 --cors",
    "test": "vitest"
  },
